  "tags": [
    {
      "name": "Sync Data"
    },
    {
      "name": "Alerts"
    },
    {
      "name": "Enrollment"
    }
  ],
  "info": {
    "version": "0.1.0",
    "title": "Chargeblast API"
  },
  "paths": {
    "/api/alerts/update/{id}": {
      "post": {
        "description": "Update the state of an alert to properly inform the banks of whether or not a refund will be issued. If an alert is left unactioned for 72 hours it will be automatically declined. For best results mark alerts as resolved within 24 hours of receiving them (some smaller banks will escalate quarantined transactions as soon as 24 hours within propogating an alert). While our API supports multiple updates, only the first response is respected by the banks.",
        "tags": ["Alerts"],
        "operationId": "alerts-update",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "format": "int64"
                }
              }
            },
            "description": "OK"
          }
        },
        "parameters": [
          {
            "name": "api_key",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            },
            "name": "id"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AlertUpdateBodyOpenAPI"
              }
            }
          },
          "required": true
        },
        "summary": "Update Alert"
      }
    },
    "/api/descriptors": {
      "get": {
        "operationId": "descriptors",
        "tags": ["Enrollment"],
        "parameters": [
          {
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "name": "api_key"
          }
        ],
        "summary": "Fetch Descriptors",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmbedDescriptorsResponseDTO"
                }
              }
            }
          }
        },
        "description": "Fetch all the descriptors for your merchants."
      }
    },
    "/api/merchant": {
      "get": {
        "tags": ["Enrollment"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PayshieldRetailerDTO"
                }
              }
            }
          }
        },
        "summary": "Fetch Merchant",
        "description": "Get an individual merchant from your chargeblast account.",
        "parameters": [
          {
            "in": "query",
            "required": true,
            "name": "api_key",
            "schema": {
              "type": "string"
            }
          },
          {
            "required": false,
            "name": "merchant_id",
            "in": "query",
            "schema": {
              "nullable": true,
              "type": "string"
            }
          }
        ],
        "operationId": "merchant"
      }
    },
    "/api/unenroll": {
      "post": {
        "operationId": "unenroll",
        "tags": ["Enrollment"],
        "summary": "Unenroll Merchant",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UnenrollmentBody"
              }
            }
          },
          "required": true
        },
        "description": "Pass in a given descriptor_id to unenroll merchant's descriptor. Ethoca alerts take 48 hours to confirm unenrollment, RDR ~2 weeks, CDRN ~24 hours.",
        "parameters": [
          {
            "required": true,
            "in": "query",
            "schema": {
              "type": "string"
            },
            "name": "api_key"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "format": "int64",
                  "type": "integer"
                }
              }
            },
            "description": "OK"
          }
        }
      }
    },
    "/api/orders/upload": {
      "post": {
        "operationId": "orders-upload",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrderUploadResponse"
                }
              }
            },
            "description": "OK"
          }
        },
        "summary": "Upload Orders",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderUploadBody"
              }
            }
          },
          "required": true
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "required": true,
            "name": "api_key",
            "in": "query"
          }
        ],
        "tags": ["Sync Data"],
        "description": "Upload orders to the chargeblast system. This will allow you to match disputes and chargebacks to the orders you have uploaded. Amount should be in cents, e.g. $19.99 => amount = 1999"
      }
    },
    "/api/merchants": {
      "get": {
        "summary": "Fetch Merchants",
        "parameters": [
          {
            "required": true,
            "in": "query",
            "name": "api_key",
            "schema": {
              "type": "string"
            }
          }
        ],
        "operationId": "merchants",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmbedMerchantResponseDTO"
                }
              }
            }
          }
        },
        "tags": ["Enrollment"],
        "description": "Get all the merchants from your chargeblast account."
      }
    },
    "/api/alerts": {
      "get": {
        "operationId": "alerts",
        "description": "Get all the alerts from your chargeblast account.",
        "summary": "Fetch Alerts",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ExportAlert"
                  }
                }
              }
            },
            "description": "OK"
          }
        },
        "parameters": [
          {
            "name": "api_key",
            "required": true,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "merchant_id",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string",
              "nullable": true
            }
          }
        ],
        "tags": ["Alerts"]
      }
    },
    "/api/enroll_merchant": {
      "post": {
        "description": "Enroll the merchant in a various alert program. If no merchant_id is specified a new merchant object will be automatically generated. For each alert program various info is required. For example, for Ethoca, only descriptors are. To see which info is required see [here](https://docs.chargeblast.io/reference/enrollment-criteria)",
        "tags": ["Enrollment"],
        "operationId": "enroll-merchant",
        "summary": "Enroll Merchant",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EmbedEnrollmentBody"
              }
            }
          }
        },
        "parameters": [
          {
            "name": "api_key",
            "required": true,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "required": false,
            "schema": {
              "nullable": true,
              "type": "string"
            },
            "name": "merchant_id"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "format": "int64",
                  "type": "integer"
                }
              }
            },
            "description": "OK"
          }
        }
      }
    },
    "/api/alert/{id}": {
      "get": {
        "tags": ["Alerts"],
        "operationId": "alert",
        "summary": "Fetch an Alert",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExportAlert"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "in": "query",
            "schema": {
              "type": "string"
            },
            "name": "api_key",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "path",
            "name": "id",
            "required": true
          }
        ],
        "description": "Get all the alerts from your chargeblast account."
      }
    }
  }
}
