            "format": "double",
            "description": "Transaction amount for the alert. $139.38 will be represented as 139.38"
          },
          "creditNotes": {
            "nullable": true,
            "description": "Additional notes or information regarding the credit status.",
            "type": "string"
          },
          "creditStatus": {
            "type": "string",
            "description": "Status of the credit (if any) associated with the alert.",
            "nullable": true
          },
          "merchantId": {
            "nullable": true,
            "type": "string",
            "description": "Identifier for the merchant within the system."
          },
          "creditAppealed": {
            "description": "Indicates if the credit associated with the alert was appealed.",
            "type": "boolean"
          },
          "resolvedByApi": {
            "description": "Indicates if the alert was resolved via API.",
            "type": "boolean",
            "nullable": true
          },
          "createdAt": {
            "description": "Date and time when the alert was created.",
            "nullable": true,
            "format": "date-time",
            "type": "string"
          },
          "provider": {
            "type": "string",
            "description": "Provider associated with the alert.",
            "nullable": true
          },
          "reasonCode": {
            "type": "string",
            "description": "Reason code associated with the alert, often indicating the nature of the dispute.",
            "nullable": true
          },
          "externalUrl": {
            "type": "string",
            "nullable": true,
            "description": "URL linking to an external reference for the alert."
          },
          "alertId": {
            "nullable": true,
            "type": "string",
            "description": "Unique identifier for the alert within the provider's system. Will match the id field."
          },
          "authCode": {
            "description": "Authorization code for the transaction, if available.",
            "type": "string",
            "nullable": true
          },
          "responseAction": {
            "type": "string",
            "nullable": true,
            "description": "Response action taken in response to the alert."
          },
          "card": {
            "description": "The redacted card number, e.g., 424242****1234.",
            "type": "string",
            "nullable": true
          },
          "acquirerAction": {
            "nullable": true,
            "description": "Actions taken by the acquirer in response to the alert.",
            "type": "string"
          },
          "descriptor": {
            "type": "string",
            "description": "Billing descriptor shown on the card statement.",
            "nullable": true
          },
          "resolvedDate": {
            "description": "Date and time when the alert was resolved.",
            "nullable": true,
            "type": "string",
            "format": "date-time"
          },
          "customerEmail": {
            "type": "string",
            "description": "Customer's email address associated with the alert.",
            "nullable": true
          },
          "currency": {
            "nullable": true,
            "type": "string",
            "description": "Currency code in ISO format (e.g., USD, EUR) for the transaction."
          },
          "customerId": {
            "type": "string",
            "description": "Unique identifier for the customer associated with the alert.",
            "nullable": true
          },
          "arn": {
            "type": "string",
            "nullable": true,
            "description": "Acquirer Reference Number, unique identifier for the transaction."
          },
          "alertType": {
            "description": "Type of alert, e.g., FRAUD, DISPUTE.",
            "type": "string"
          },
          "cardBrand": {
            "description": "The card network (e.g., Visa, Mastercard, Amex).",
            "type": "string",
            "nullable": true
          }
        },
        "description": "Export alert details.",
        "required": ["alertType", "creditAppealed", "subprovider"],
        "type": "object"
      },
      "ProviderSource": {
        "enum": ["Banks", "Ethoca", "Verifi"],
        "type": "string"
      },
      "PayshieldDescriptorDescriptorStatus": {
        "type": "string",
        "enum": [
          "Pending",
          "Enrolled",
          "Disabled",
          "Surpressed",
          "Processing",
          "Rejected",
          "InReview"
        ]
      },
      "ShopifyIntegration": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "external_id": {
            "type": "string",
            "nullable": true
          },
          "store_unique_name": {
            "nullable": true,
            "type": "string"
          },
          "state": {
            "nullable": true,
            "type": "string"
          }
        }
      },
      "RuleGroup": {
        "type": "object",
        "required": ["logicOperator"],
        "properties": {
          "logicOperator": {
            "type": "string"
          }
        }
      },
      "UserDTO": {
        "type": "object",
        "required": [
          "accounts",
          "agentDeal",
          "announcements",
          "invitations",
          "tokens"
        ],
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "internalId": {
            "nullable": true,
            "type": "string"
          },
          "pfStripeId": {
            "nullable": true,
            "type": "string"
          },
          "svixId": {
            "type": "string",
            "nullable": true
          },
          "stripeId": {
            "type": "string",
            "nullable": true
          },
          "fromInvite": {
            "type": "boolean",
            "nullable": true
          },
          "isBilledForResolved": {
            "type": "boolean",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "isSalesAgent": {
            "type": "boolean",
            "nullable": true
          },
          "merchantId": {
            "type": "string",
            "nullable": true
          },
          "invitations": {
            "items": {
              "$ref": "#/components/schemas/InvitationDTO"
            },
            "type": "array"
          },
          "announcements": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AnnouncementDTO"
            }
          },
          "settings": {
            "$ref": "#/components/schemas/UserSettingsDTO"
          },
          "phonenumber": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "utmSource": {
            "nullable": true,
            "type": "string"
          },
          "internalCompany": {
            "nullable": true,
            "type": "string"
          },
          "hubspotId": {
            "nullable": true,
            "type": "string"
          },
          "tokens": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RutterTokenDTO"
            }
          },
          "company": {
            "type": "string",
            "nullable": true
          },
          "risk": {
            "$ref": "#/components/schemas/AccountRiskLevel"
          },
          "createdAt": {
            "nullable": true,
            "type": "string",
            "format": "date-time"
          },
          "agentDeal": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AgentDealDTO"
            }
          },
          "admin": {
            "nullable": true,
            "type": "boolean"
          },
          "referralCode": {
            "type": "string",
            "nullable": true
          },
          "accounts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CBAccountDTO"
            }
          }
        }
      },
      "AccountRiskLevel": {
        "type": "string",
        "enum": ["High", "Medium", "Low"]
      },
      "InvitationDTO": {
        "type": "object",
        "properties": {
          "status": {
            "$ref": "#/components/schemas/InvitationDTOInvitationStatus"
          },
          "email": {
            "type": "string"
          },
          "permission": {
            "$ref": "#/components/schemas/AccountPermission"
          },
          "id": {
            "nullable": true,
            "type": "string"
          },
          "createdAt": {
            "nullable": true,
            "format": "date-time",
            "type": "string"
          }
        },
        "required": ["email", "permission"]
      },
      "OrderUploadBodyOrder": {
        "type": "object",
        "properties": {
          "amount": {
            "format": "int64",
            "type": "integer"
          },
          "transactionDate": {
            "type": "string"
          },
          "last4": {
            "nullable": true,
            "type": "string"
          },
          "arn": {
            "nullable": true,
            "type": "string"
          },
          "descriptor": {
            "type": "string",
            "nullable": true
          },
          "id": {
            "type": "string"
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "nullable": true,
            "type": "string"
          },
          "bin": {
            "type": "string",
            "nullable": true
          },
          "refunded": {
            "nullable": true,
            "type": "boolean"
          },
          "gateway": {
            "nullable": true,
            "type": "string"
          },
          "currency": {
            "type": "string"
          },
          "authCode": {
            "type": "string",
            "nullable": true
          }
        },
        "required": ["amount", "currency", "id", "transactionDate"]
      },
      "AnnouncementDTOAnnouncementType": {
        "type": "string",
        "enum": ["Error", "Warning", "Success"]
      },
      "AgentSettings": {
        "type": "object",
        "properties": {
          "cryptoInfo": {
            "$ref": "#/components/schemas/AgentSettingsCryptoInfo"
          },
          "bankTransferInfo": {
            "$ref": "#/components/schemas/AgentSettingsBankTransferInfo"
          }
        },
        "required": ["bankTransferInfo", "cryptoInfo"]
      },
      "VerifiRiskLevel": {
        "type": "string",
        "enum": ["High", "Medium", "Low"]
      },
      "StripeIntegration": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean"
          },
          "state": {
